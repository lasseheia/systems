{
  "mainBar": {
    "modules-left": [
      "hyprland/workspaces",
      "network"
    ],
    "modules-center": [
      "clock"
    ],
    "modules-right": [
      "battery",
      "backlight",
      "cpu",
      "memory",
      "disk#root",
      "disk#home",
      "pulseaudio",
      "bluetooth"
    ],
    "backlight": {
      "device": "intel_backlight",
      "format": "{icon}  {percent}%",
      "format-icons": [
        "",
        ""
      ]
    },
    "battery": {
      "interval": 60,
      "states": {
        "warning": 30,
        "critical": 15
      },
      "format-icons": [
        "",
        "",
        "",
        "",
        ""
      ],
      "format": "{icon}  {capacity}%",
      "format-charging": "{icon}  {capacity}% "
    }
  }
}
